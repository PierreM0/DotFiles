
#git
alias gf='git fetch'
alias gp='git pull'
alias gP='git push'

#file exploring
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias .....='cd ../../../..'
alias d='cd ~/Documents'
alias dl='cd ~/Downloads'
alias dk='cd ~/Desktop'
alias gt='cd ~/git'

#ls colorized
alias ls='ls --color'
alias ll='ls -lF --color'
alias la='ls -a --color'
alias lal='ls -lAF --color'

#grep colorized
alias grep='grep --color=auto'
alias fgrep='fgrep --color=auto'
alias egrep='egrep --color=auto'

#vim
alias vi='vim'
